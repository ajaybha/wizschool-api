
model User {
  id          String     @id @default(auto()) @map("_id") @db.ObjectId
  address     String     @unique
  email       String?     
  name        String?
  walletType  WalletType   @default(User)
  
  // relation fields
  collection  Collection[] 
  assets      Asset[]
}

enum WalletType {
  User
  Contract
}